        return translation
    
    def DeeplTranslor(txt, out, src):
        DeeplTranslor = DeeplTranslate()
        translation = DeeplTranslor.translate(txt, out, src)
        print("Deepl")
        return translation
    
    def MicrosoftTranslator(txt, out, src):
        MicrosoftTranslator = MicrosoftTranslate()
